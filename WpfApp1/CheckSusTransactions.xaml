<Window x:Class="WpfApp1.CheckSusTransactions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Podejrzane transakcje" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="LstSusTrn" HorizontalAlignment="Center" Margin="0,50,0,0" Height="141" Width="700" VerticalAlignment="Top"/>
        <Button Content="Anuluj" Click="Click_Back" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,10" Height="30" Width="150"/>
        <Button Content="Zatwierdź" Click="Click_ApproveTransaction" Margin="0,0,10,15" HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="30" Width="150"/>

    </Grid>
</Window>
